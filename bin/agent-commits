#!/usr/bin/env node
// Support both ESM and CJS builds
(async () => {
  try {
    // Prefer ESM build when available
    await import('../dist/cli.js');
  } catch (err) {
    try {
      // Fallback to CommonJS build
      require('../dist/cli.cjs');
    } catch (err2) {
      const msg = (err2 && err2.message) || (err && err.message) || String(err2 || err);
      console.error('[agent-commits] Failed to start CLI.', msg);
      process.exit(1);
    }
  }
})();

